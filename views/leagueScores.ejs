<!DOCTYPE html>
<html>
  <%- include('./navs'); -%>
            <h2>League Scores</h2>
            <div class="row">
              <div class="col">
                <ul>
                  <%_ for (const team of teams) { -%>
                    <li><%= team.name-%></li>
                  <%_ } -%>
                </ul>
              </div>
              <div class="col">
                <ul>
                  <%_ for (const team of teams) { -%>
                    <li><%= team.score-%></li>
                  <%_ } -%>
                </ul>
              </div>

            </div>

</html>
